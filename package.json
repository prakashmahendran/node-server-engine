{
  "name": "node-server-engine",
  "version": "1.11.0",
  "description": "Framework used to develop backend services. This package ships with a lot of features to standardize the creation of services, letting you focus on the business logic.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && shx mv dist/src/* dist && rimraf dist/src && tsc-alias -p tsconfig.build.json && npx babel src --out-dir dist --source-maps --extensions \".ts\" --ignore \"**/*.test.ts\" --copy-files --no-copy-ignored",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublish": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha --require ts-node/register --require tsconfig-paths/register --reporter spec --timeout 10000 --exit --full-trace test/hooks.ts 'src/**/*.test.ts'",
    "test:coverage": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json nyc --reporter=text --no-clean mocha --require ts-node/register --require tsconfig-paths/register --reporter spec --full-trace test/hooks.ts 'src/**/*.test.ts'",
    "test:file": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha --require ts-node/register --require tsconfig-paths/register --reporter spec --exit test/hooks.ts",
    "coverage": "nyc report --report-dir=./.test-coverage --reporter=html --reporter=text",
    "coverage:ci": "nyc report --reporter=text-summary",
    "test:ci": "npm run test -- --reporter spec",
    "test:compose": "docker-compose up --abort-on-container-exit --build",
    "prepare": "husky"
  },
  "keywords": [
    "node",
    "express",
    "server",
    "backend",
    "framework",
    "rest-api",
    "typescript",
    "microservices",
    "jwt",
    "websocket",
    "sequelize"
  ],
  "author": "Ram",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prakashmahendran/node-server-engine.git"
  },
  "bugs": {
    "url": "https://github.com/prakashmahendran/node-server-engine/issues"
  },
  "homepage": "https://github.com/prakashmahendran/node-server-engine#readme",
