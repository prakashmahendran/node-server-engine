{
  "all": true,
  "check-coverage": true,
  "exclude": [
    "dist/**",
    "certs/**",
    "test/**",
    "coverage/**",
    "src/db/migrations/**",
    "env/**",
    "src/index.js",
    ".commitlintrc.js",
    ".releaserc.js"
  ],
  "skip-full": true,
  "sourceMap": true,
  "instrument": true,
  "require": ["@babel/register"],
  "reporter": ["text", "html", "lcov"],
  "lines": 70,
  "branches": 70,
  "functions": 70,
  "statements": 70,
  "report-dir": "./.test-coverage",
  "temp-dir": "./.nyc_output"
}
